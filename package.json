{
  "name": "webpack-library-starter",
  "version": "0.1.3",
  "description": "Produce universal library with webpack and es6",
  "main": "./build/Library.js",
  "scripts": {
    "start": "npm run develop",
    "develop": "webpack --progress --colors --watch --env=development",
    "build": "webpack --env=production --optimize-minimize",
    "test": "mocha --compilers js:babel-core/register -r jsdom-global/register --colors -w ./test/*.spec.js"
  },
  "devDependencies": {
    "babel": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-helper-vue-jsx-merge-props": "latest",
    "babel-loader": "latest",
    "babel-plugin-add-module-exports": "latest",
    "babel-plugin-syntax-jsx": "latest",
    "babel-plugin-transform-async-to-generator": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-plugin-transform-vue-jsx": "latest",
    "babel-preset-es2015": "latest",
    "babel-register": "latest",
    "chai": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "latest",
    "jsdom": "latest",
    "jsdom-global": "latest",
    "lodash": "latest",
    "mocha": "latest",
    "vue": "latest",
    "vue-loader": "latest",
    "webpack": "latest",
    "yargs": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/webpack-library-starter.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krasimir/webpack-library-starter/issues"
  },
  "homepage": "https://github.com/krasimir/webpack-library-starter"
}
